<h1 class="login">Welcome Back! Login Below...</h1>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, class: "login-labels" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "login-fill" %>
  </div>

  <div class="field">
    <%= f.label :password, class: "login-labels" %><br />
    <%= f.password_field :password, autocomplete: "current-password", class: "login-fill" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: "login-btn" %>
  </div>
<% end %>

<div class ="login-reg">
<%= render "devise/shared/links" %>
</div>
